{
  "name": "Mytickets",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "npx vue-cli-service serve",
    "build": "npx vue-cli-service build --skip-plugins @vue/cli-plugin-eslint",
    "lint": "npx vue-cli-service lint",
    "release": "git tag v$npm_package_version && git push && git push --tags"
  },
  "dependencies": {
    "@types/node": "^16.4.10",
    "@types/node-fetch": "^2.5.12",
    "@vanroeybe/vue-inversify-plugin": "^1.1.3",
    "core-js": "^3.15.2",
    "inversify": "^5.1.1",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2",
    "vee-validate": "^3.4.11",
    "vue": "2.6.14",
    "vue-barcode-reader": "^0.0.3",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.2",
    "vuetify": "2.5.7",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vue/babel-preset-app": "^4.5.13",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-typescript": "~4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-typescript": "7.0.0",
    "babel-core": "^6.26.3",
    "eslint": "7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.14.0",
    "jest": "^27.0.6",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.2",
    "sass": "~1.32.6",
    "sass-loader": "^10.1.1",
    "ts-jest": "^27.0.4",
    "typescript": "~4.3.5",
    "vue-cli-plugin-vuetify": "~2.4.1",
    "vue-docgen-loader": "^1.5.0",
    "vue-template-compiler": "2.6.14",
    "vuetify-loader": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "plugins": [
        "@typescript-eslint/eslint-plugin"
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
